<h1>Listing event_type_answer_weights</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Potential answer</th>
      <th>Event type</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_type_answer_weights.each do |event_type_answer_weight| %>
      <tr>
        <td><%= event_type_answer_weight.id %></td>	
        <td><%= event_type_answer_weight.potential_answer.answer %></td>
        <td><%= event_type_answer_weight.event_type.event_type_name %></td>
        <td><%= event_type_answer_weight.weight %></td>
        <td><%= link_to 'Show', event_type_answer_weight %></td>
        <td><%= link_to 'Edit', edit_event_type_answer_weight_path(event_type_answer_weight) %></td>
        <td><%= link_to 'Destroy', event_type_answer_weight, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event type answer weight', new_event_type_answer_weight_path %>

<h2>Import weights</h2>

<%= form_tag import_event_type_answer_weights_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>

<ul class="nav pull-right">	
  <li><%= link_to "Home",         root_path %></li>
  <% if user_signed_in? %>
  <li><%= link_to "Profile", edit_user_registration_path %></li>
  <li><%= link_to "Logout", destroy_user_session_path, :method => :delete %></li>
  <% else %>
  <li><%= link_to "Sign in", new_user_session_path %></li>
  <% end %>
</ul>
